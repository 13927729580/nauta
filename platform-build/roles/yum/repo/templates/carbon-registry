Summary: Carbon docker registry repository
Name: carbon-registry-repository
Version: {{ build_major }}.{{ build_minor }}.{{ build_no }}
Release: {{ build_id }}
License: Apache-2.0
Group: Tools
SOURCE0: docker.tar.gz

BuildRoot: %{_tmppath}/%{name}-%{version}-%{release}-root

%define  debug_package %{nil}

%description
%{summary}

%prep
%setup -q -n docker

%build
# Empty section.

%install
rm -rf %{buildroot}
mkdir -p  %{buildroot}

mkdir -p %{buildroot}/opt/carbon/registry/docker
cp -a * %{buildroot}/opt/carbon/registry/docker/

exit 0
%clean
rm -rf %{buildroot}


%files
%defattr(-,root,root,-)
/opt/carbon/registry/docker/*
